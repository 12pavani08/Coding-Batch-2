import java.io.*;
import java.util.*;
class Solution
{
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		int t=sc.nextInt();
		while((t>=0)&&(t<=500))
		{
			int a=sc.nextInt();
			int b=sc.nextInt();
			int n=sc.nextInt();
			
			int ans=a;
			if(((a>=0)&&(a<=50)) && ((b>0)&&(b<=50)) && ((n>=1)&&(n<=15)))
			{
				for(int i=0;i<n;i++)
				{
				ans+=Math.pow(2,i)*b;
				System.out.print(ans+" ");
				}
				System.out.println();
			}
		}	
	}
}
