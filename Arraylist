public static void main(String args[])
{
ArrayList p1=new ArrayList();
p1.add("10");
p1.add("20");
p1.add("30");
p1.add("40");
p1.add("50");
{
ArrayList p2=new ArrayList();
p2.add("100");
p2.add("200");
p2.add("300");
p2.add("400");
p2.add("500");
{
ArrayList p3=new ArrayList();
p3.add("10,20,30,40,50,100,200,300,400,500");
{
Collections.sort(p3);
{
System.out.println("enter array after sorting:"+p3);
}
}
}
}
}
}
